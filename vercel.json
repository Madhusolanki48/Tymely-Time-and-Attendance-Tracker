{
  "version": 2,
  "builds": [
    {
      "src": "attendance_system/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "attendance_system/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False",
    "DJANGO_SETTINGS_MODULE": "attendance_system.settings"
  },
  "functions": {
    "attendance_system/wsgi.py": {
      "maxDuration": 30
    }
  }
}
